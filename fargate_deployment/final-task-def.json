{
  "family": "patient-pipeline-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::665277163763:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "patient-pipeline",
      "image": "665277163763.dkr.ecr.us-east-1.amazonaws.com/patient-pipeline:latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/patient-pipeline",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "INPUT_S3",
          "value": "s3://medical-note-llm/Input/may_01_14.csv"
        },
        {
          "name": "OUTPUT_S3",
          "value": "s3://medical-note-llm/Output/output_assessment.csv"
        },
        {
          "name": "EMAIL_FROM",
          "value": "timfwater@gmail.com"
        },
        {
          "name": "EMAIL_TO",
          "value": "timfwater@gmail.com"
        },
        {
          "name": "THRESHOLD",
          "value": "0.94"
        },
        {
          "name": "START_DATE",
          "value": "2024-05-01"
        },
        {
          "name": "END_DATE",
          "value": "2024-05-07"
        },
        {
          "name": "ALERT_TOPIC_ARN",
          "value": "arn:aws:sns:us-east-1:665277163763:patient-pipeline-alerts"
        },
        {
          "name": "PHYSICIAN_ID_LIST",
          "value": "1"
        },
        {
          "name": "AUDIT_BUCKET",
          "value": "medical-note-llm"
        },
        {
          "name": "AUDIT_PREFIX",
          "value": "audit_logs"
        }
      ]
    }
  ],
  "taskRoleArn": "arn:aws:iam::665277163763:role/ecsTaskRole"
}